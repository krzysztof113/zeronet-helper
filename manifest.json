{

  "description": "Zeronet helper makes easy accessing zeronet sites",
  "manifest_version": 2,
  "name": "Zeronet Helper",
  "version": "0.5",
  "icons": {
    "48": "icons/zeronet-logo.svg"
  },

  "applications": {
    "gecko": {
      "id": "zeronet_helper@krzysztof113",
      "strict_min_version": "50.0"
    }
  },
  "background": {
    "scripts": ["jquery.min.js", "lib.js", "background.js"]
  },

  "browser_action": {
    "default_icon": "icons/zeronet-logo.svg"
  },
  
  "permissions": ["notifications", 
    "nativeMessaging", "http://127.0.0.1/*", 
    "webRequest", "webRequestBlocking", "tabs", "storage"],
    "options_ui": {
      "page": "options.html"
    }
}
